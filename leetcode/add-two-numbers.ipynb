   "source": [
    "class ListNode:\n",
    "    def __init__(self, val=0, next=None):\n",
    "        self.val = val\n",
    "        self.next = next\n",
    "    def __repr__(self) -> str:\n",
    "        return f'{self.val}, next: {self.next if not None else \"\"}'\n"
   ]
